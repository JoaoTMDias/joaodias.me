{
	"name": "joaodias.me",
	"description": "joaodias-me website",
	"version": "1.0.0",
	"author": "Joao Dias <hello@joaodias.me>",
	"dependencies": {
		"@loadable/component": "^5.12.0",
		"@testing-library/jest-dom": "^5.5.0",
		"@testing-library/react": "^10.0.4",
		"axios": "^0.19.2",
		"babel-jest": "^26.0.1",
		"bluebird": "^3.7.2",
		"classnames": "^2.2.6",
		"formik": "^2.1.4",
		"gatsby": "2.21.15",
		"gatsby-cli": "^2.12.11",
		"gatsby-image": "^2.4.2",
		"gatsby-plugin-manifest": "2.4.2",
		"gatsby-plugin-netlify": "^2.3.2",
		"gatsby-plugin-offline": "^3.2.1",
		"gatsby-plugin-react-helmet": "3.3.1",
		"gatsby-plugin-sass": "2.3.1",
		"gatsby-plugin-sharp": "^2.6.1",
		"gatsby-plugin-sitemap": "2.4.2",
		"gatsby-plugin-styled-components": "3.3.1",
		"gatsby-plugin-typescript": "^2.4.2",
		"gatsby-source-contentful": "^2.3.2",
		"gatsby-source-filesystem": "^2.3.1",
		"gatsby-transformer-remark": "2.8.7",
		"gatsby-transformer-sharp": "^2.5.1",
		"intersection-observer": "^0.10.0",
		"js-logger": "^1.6.0",
		"node-sass": "^4.14.1",
		"p-min-delay": "^3.1.0",
		"polished": "^3.6.2",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-helmet": "^6.0.0",
		"react-spring": "^9.0.0-beta.34",
		"react-with-gesture": "^4.0.8",
		"shallowequal": "^1.1.0",
		"styled-components": "^5.1.0",
		"supports-webp": "^2.0.1",
		"yup": "^0.28.5"
	},
	"keywords": [
		"gatsby"
	],
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf node_modules && rm -rf ./.cache",
		"rebuild": "npm run clean && npm run install",
		"build": "gatsby build",
		"start": "gatsby develop",
		"develop": "gatsby develop",
		"prettier:base": "prettier --parser typescript",
		"format:fix": "npm run prettier:base -- --write 'src/**/*.{ts,tsx, js, jsx, json}'",
		"format:lint": "eslint --fix src/**/*.tsx",
		"test": "jest --env=jsdom",
		"test:watch": "jest --watchAll --env=jsdom",
		"test:cypress": "cypress run",
		"test:cypress-ui": "cypress open",
		"type-check": "tsc --noEmit",
		"prebuild": "npm run format:fix && npm run format:lint && npm run test"
	},
	"devDependencies": {
		"@4tw/cypress-drag-drop": "^1.3.1",
		"@babel/core": "^7.9.6",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.9.6",
		"@babel/plugin-proposal-optional-chaining": "^7.9.0",
		"@babel/polyfill": "^7.8.7",
		"@babel/preset-env": "^7.9.6",
		"@babel/preset-typescript": "^7.9.0",
		"@cypress/code-coverage": "^3.7.0",
		"@types/classnames": "^2.2.10",
		"@types/jest": "^25.2.1",
		"@types/node": "^13.13.5",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.7",
		"@types/react-helmet": "^5.0.15",
		"@types/shallowequal": "^1.1.1",
		"@types/styled-components": "^5.1.0",
		"@types/yup": "^0.28.0",
		"@typescript-eslint/eslint-plugin": "^2.31.0",
		"@typescript-eslint/parser": "^2.31.0",
		"babel-plugin-styled-components": "^1.10.7",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babel-preset-gatsby": "^0.4.1",
		"cypress": "^4.5.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-import-resolver-typescript": "^2.0.0",
		"eslint-plugin-chai-friendly": "^0.6.0",
		"eslint-plugin-cypress": "^2.10.3",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jest": "^23.9.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-react-hooks": "^4.0.0",
		"husky": "^4.2.5",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^26.0.1",
		"lint-staged": "^10.2.2",
		"prettier": "^2.0.5",
		"ts-jest": "^25.5.0",
		"typescript": "^3.8.3",
		"typescript-styled-plugin": "^0.15.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JoaoTMDias/joaodias.me"
	},
	"browserslist": [
		">1.5%",
		"not dead",
		"not op_mini all"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier --write",
			"git add"
		]
	},
	"jest": {
		"setupFilesAfterEnv": [
			"@testing-library/jest-dom",
			"@testing-library/jest-dom/extend-expect",
			"./setup-jest.js"
		],
		"globals": {
			"__PATH_PREFIX__": ""
		},
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.(tsx?|jsx?)$": "ts-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(gatsby)/)",
			"static"
		],
		"testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
		"testPathIgnorePatterns": [
			"node_modules",
			".cache",
			"static"
		],
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx"
		],
		"moduleNameMapper": {
			"typeface-*": "identity-obj-proxy",
			".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
			".+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
		},
		"collectCoverage": true,
		"coverageReporters": [
			"lcov",
			"text",
			"html"
		]
	}
}
